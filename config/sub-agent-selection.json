      "secondary": [
        "RLS", "policy", "user", "admin", "privileges", "audit"
      ],
      "exclusions": [
        "security deposit", "job security", "security guard"
      ],
      "minMatches": 2,
      "weight": { "title": 3, "description": 2, "content": 1 }
    },

    "DESIGN": {
