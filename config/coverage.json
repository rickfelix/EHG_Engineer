{
  "threshold": 35,
  "reporters": ["json-summary", "lcov", "text"],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/**/*.test.{js,jsx}",
    "!src/client/dist/**"
  ],
  "temporaryBypass": {
    "enabled": true,
    "reason": "Building coverage incrementally during ESM migration",
    "targetThreshold": 75
  }
}