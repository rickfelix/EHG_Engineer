{
  "name": "ehg-engineer",
  "version": "1.0.0",
  "description": "LEO Protocol development environment with Claude Code multi-agent workflow support",
  "scripts": {
    "check-db": "node scripts/verify-connection.js",
    "setup-db": "node scripts/setup-database.js",
    "setup-db-supabase": "node scripts/setup-database-supabase.js",
    "check-directives": "node scripts/check-directives-data.js",
    "update-status": "node scripts/update-directive-status.js",
    "audit-compliance": "bash scripts/leo_compliance_audit.sh",
    "test-database": "node scripts/test-database.js",
    "new-sd": "node scripts/new-strategic-directive.js",
    "add-sd": "node scripts/add-sd-to-database.js",
    "register-app": "node scripts/register-app.js",
    "switch-context": "node scripts/switch-context.js",
    "show-context": "node scripts/switch-context.js show",
    "list-apps": "node scripts/switch-context.js list",
    "encrypt-test": "node lib/security/encryption.js validate",
    "sync-github": "node scripts/sync-github.js",
    "sync-supabase": "node scripts/sync-supabase.js",
    "sync-app": "node scripts/sync-manager.js",
    "create-app-sd": "node scripts/create-app-sd.js",
    "deploy-sd": "node scripts/deploy-sd-to-app.js",
    "leo": "node scripts/leo.js",
    "leo-status": "node scripts/leo-status-line.js show",
    "build:loader": "tsc -p tsconfig.json",
    "test:loader:dry": "node dist/services/database-loader/index.js --dry-run",
    "test:coverage": "jest --coverage --coverageReporters=json-summary --coverageDirectory=coverage",
    "test:a11y": "playwright test tests/a11y.spec.js",
    "analyze:bundle": "esbuild src/client/src/main.jsx --bundle --metafile=meta.json --outfile=/dev/null && esbuild-visualizer --metadata meta.json",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.json",
    "build:client": "cd src/client && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "openai": "^5.22.0",
    "p-retry": "^7.0.0",
    "playwright": "^1.40.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@lhci/cli": "^0.13.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.5",
    "babel-jest": "^30.1.2",
    "esbuild": "^0.25.10",
    "esbuild-visualizer": "^0.6.0",
    "jest": "^29.7.0",
    "pg": "^8.16.3",
    "typescript": "^5.9.2",
    "wait-on": "^7.0.1"
  }
}
