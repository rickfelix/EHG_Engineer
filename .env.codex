# Codex Lane Environment (Read-Only)
# This file contains credentials for the Codex builder lane
# LIMITED to read-only operations

# Supabase Configuration (Read-Only)
SUPABASE_URL=https://dedlbzhpgkmetvhbkyzq.supabase.co
SUPABASE_ANON_KEY=# Add anon key here (read-only access)

# Node Environment
NODE_ENV=development

# Application Settings
PORT=3001  # Different port to avoid conflicts with Claude lane

# Feature Flags
ENABLE_CODEX_MODE=true
ENABLE_WRITE_OPERATIONS=false
ENABLE_DATABASE_WRITES=false

# Security Settings
READ_ONLY_MODE=true
ALLOW_MIGRATIONS=false
ALLOW_SCHEMA_CHANGES=false

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Build Settings
BUILD_EPHEMERAL=true
BUILD_CACHE_SHARING=false
BUILD_PARAMETERLESS=true

# Artifact Settings
ARTIFACT_FORMAT=tar.gz
SBOM_FORMAT=cyclonedx-1.5
PROVENANCE_FORMAT=in-toto-v1.0
PROVENANCE_PREDICATE=slsaprovenance/v0.2

# Branch Restrictions
ALLOWED_BRANCH_PREFIX=staging/codex-
FORBIDDEN_BRANCH_PREFIX=feature/,main,develop,master

# Commit Markers
READY_MARKER=[CODEX-READY]
HANDOFF_REQUIRED=true